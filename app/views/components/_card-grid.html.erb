<div class="cards">
  <%@cocktails.each do |cocktail|%>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1559443922-3f698a0ff27a?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=2769&q=80)">
      <%=link_to cocktail.name, cocktail_path(cocktail), class: "link"%>
    </div>
  <%end%>
</div>
